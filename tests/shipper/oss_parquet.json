{
  "shipperName": "oss-ship-test1",
  "targetConfiguration": {
    "bufferInterval": 300,
    "bufferSize": 256,
    "compressType": "snappy",
    "enable": true,
    "ossBucket": "bucket1",
    "ossPrefix": "bucket_folder1",
    "pathFormat": "%Y/%m/%d/%H/%M",
    "roleArn": "acs:ram::1234:role/aliyunlogdefaultrole",
    "storage": {
      "detail": {
        "columns": [
          {
            "name": "v1",
            "type": "string"
          },
          {
            "name": "v2",
            "type": "string"
          }
        ]
      },
      "format": "parquet"
    }
  },
  "targetType": "oss"
}
